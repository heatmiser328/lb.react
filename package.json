{
  "name": "lb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install-dev": "react-native run-android",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "get-js": "curl \"http://localhost:8081/index.android.bundle?platform=android&dev=false&minify=true\" -o \"android/app/src/main/assets/index.android.bundle\"",
    "assemble": "cd android && gradlew assembleRelease && cd ..",
    "install-dbg": "adb install -r android/app/build/outputs/apk/lb-debug-1.0.apk",
    "install-rel": "adb install -r android/app/build/outputs/apk/lb-release-1.0.apk",
    "bridge": "adb reverse tcp:8081 tcp:8081",
    "menu": "adb shell input keyevent 82",
    "logs": "adb logcat *:S ReactNative:V ReactNativeJS:V"
  },
  "dependencies": {
    "apsl-react-native-button": "^2.4.2",
    "autobind-decorator": "^1.3.2",
    "moment": "^2.10.6",
    "react": "^0.14.7",
    "react-native": "^0.22.2",
    "react-native-audioplayer": "^0.2.0",
    "react-native-fs": "^1.2.0",
    "react-native-scrollable-tab-view": "^0.4.0",
    "react-native-simple-radio-button": "^1.4.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.3",
    "sandboxed-module": "^2.0.3",
    "sinon": "^1.17.2"
  }
}
